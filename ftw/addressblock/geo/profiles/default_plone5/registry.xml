<?xml version="1.0"?>
<registry>

    <records interface="collective.geo.settings.interfaces.IGeoFeatureStyle">
        <value key="map_viewlet_position">plone.belowcontentbody</value>
    </records>

    <records prefix="plone.resources/addressblock-geo-js"
             interface="Products.CMFPlone.interfaces.IResourceRegistry">
        <value key="js">++resource++ftw.addressblock.geo/addressblock.js</value>
        <value key="deps">jquery</value>
    </records>

    <records prefix="plone.bundles/ftw-addressblock-geo-resources" interface="Products.CMFPlone.interfaces.IBundleRegistry">
        <value key="resources">
            <element>addressblock-geo-js</element>
        </value>
        <value key="enabled">True</value>
        <value key="depends">plone-legacy</value>
        <value key="compile">False</value>
        <value key="jscompilation">++plone++addressblock/ftw-addressblock-geo-compiled.js</value>
        <value key="merge_with">default</value>
    </records>

</registry>
